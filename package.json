{
  "name": "@unscatty/unocss-preset-daisy",
  "version": "0.0.4",
  "description": "UnoCSS preset for daisyUI",
  "repository": "unscatty/unocss-preset-daisy",
  "author": "Carlos Aguilera",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run generate:compile && pnpm run generate && unbuild --minify && pnpm run clean",
    "build:dev": "pnpm run build && pnpm run dev",
    "dev": "vite playground",
    "play": "jiti playground",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "release": "changelogen --release && npm publish --access public && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck",
    "generate": "mkdirp src/generated && node src/generate/dist/generate.mjs",
    "generate:compile": "unbuild src/generate",
    "generate:compile:clean": "rimraf src/generate/dist/",
    "generate:clean": "rimraf src/generated/",
    "clean": "pnpm run generate:clean && pnpm run generate:compile:clean"
  },
  "devDependencies": {
    "@iconify-json/octicon": "^1.1.49",
    "@types/node": "^20.5.4",
    "@types/postcss-js": "^4.0.2",
    "@unocss/reset": "^0.56.4",
    "@vitest/coverage-v8": "^0.34.2",
    "changelogen": "^0.5.5",
    "daisyui": "^3.8.3",
    "eslint": "^8.47.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.19.3",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "unbuild": "^2.0.0",
    "unocss": "^0.56.4",
    "vite": "^4.4.9",
    "vitest": "^0.34.2"
  },
  "packageManager": "pnpm@8.6.12",
  "dependencies": {
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "camelcase": "^8.0.0",
    "mkdirp": "^3.0.1",
    "parsel-js": "^1.1.2",
    "postcss": "^8.4.31",
    "postcss-js": "^4.0.1",
    "postcss-values-parser": "^6.0.2",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "daisyui": "^3.8.3",
    "unocss": "^0.56.4"
  }
}
